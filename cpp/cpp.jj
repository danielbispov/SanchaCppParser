TOKEN :
{
  < LCURLYBRACE: "{" >
| < RCURLYBRACE: "}" >
| < LPARENTHESIS: "(" >
| < RPARENTHESIS: ")" >
| < SEMICOLON: ";" >
| < ASSIGNEQUAL: "=" >
| < TIMESEQUAL: "*=" >
| < DIVIDEEQUAL: "/=" >
| < MODEQUAL: "%=" >
| < PLUSEQUAL: "+=" >
| < MINUSEQUAL: "-=" >
| < OR: "||" >
| < AND: "&&" >
| < EQUAL: "==" >
| < NOTEQUAL: "!=" >
| < LESSTHAN: "<" >
| < GREATERTHAN: ">" >
| < LESSTHANOREQUALTO: "<=" >
| < GREATERTHANOREQUALTO: ">=" >
| < PLUS: "+" >
| < MINUS: "-" >
| < STAR: "*" >
| < DIVIDE: "/" >
| < MOD: "%" >
| < PLUSPLUS: "++" >
| < MINUSMINUS: "--" >
| < CHAR: "char" >
| < CONST: "const" >
| < DO: "do" >
| < DOUBLE: "double" >
| < ELSE: "else" >
| < FLOAT: "float" >
| < FOR: "for" >
| < IF: "if" >
| < INT: "int" >
| < RETURN: "return" >
| < CASE: "case" >
| < SWITCH: "switch" >
| < VOID: "void" >
| < WHILE: "while" >
| < TRUETOK: "true" >
| < FALSETOK: "false" >
}


SKIP :
{
  " "
  |
  "\t"
  |
  "\r\n"
  |
  "\n"
  |
  "//" : COMENTARIO_LINHA
  |
  "/*" : COMENTARIO_BLOCO
}

<COMENTARIO_LINHA> SKIP:
{
  "\n" : DEFAULT
}
<COMENTARIO_BLOCO> SKIP:
{
  "*/" : DEFAULT
}
